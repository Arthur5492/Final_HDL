module Mux4x1_1bit
(
	level,
	CL1,
	CL2,
	CL3,
	CL4,
	CLKHZ
);
	localparam P_level = 2;
	
	// Input Port(s)
	input wire [p_level-1:0] level;
	input wire CLI1;
	input wire CLI2;
	input wire CLI3;
	input wire CLI4;
	
	// Output Port(s)
	output wire CLKHZ;
	
	
	// Parameter Declaration(s)
	parameter <param_name> = <default_value>;
	parameter [<msb>:<lsb>] <param_name> = <default_value>;
	parameter signed [<msb>:<lsb>] <param_name> = <default_value>;
	...

	// Additional Module Item(s)

endmodule
